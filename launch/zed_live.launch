<launch>

	<rosparam command="load" file="$(env HOME)/workspace/data/bags/stereo/zed/cam0.yaml" ns="/cam0"/>
	<rosparam command="load" file="$(env HOME)/workspace/data/bags/stereo/zed/cam1.yaml" ns="/cam1"/>

	<node name="zed_split" pkg="direct_stereo" type="zed_split" args="0" output="screen" />

	 <node name="manager_node" pkg="direct_stereo" type="manager_node" output="screen" >
	    <param name="cam0_topic" value="/zed/left/image_raw_color" />
	    <param name="cam1_topic" value="/zed/right/image_raw_color" />
	    <param name="gt_type" value="odom" />
    </node>

</launch>
