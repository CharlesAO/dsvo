<launch>

	<rosparam command="load" file="$(env HOME)/workspace/data/bags/stereo/v1/cam0/sensor.yaml" ns="/cam0"/>
	<rosparam command="load" file="$(env HOME)/workspace/data/bags/stereo/v1/cam1/sensor.yaml" ns="/cam1"/>

	 <node name="manager_node" pkg="direct_stereo" type="manager_node" output="screen" >
	    <param name="cam0_topic" value="/cam0/image_raw" />
	    <param name="cam1_topic" value="/cam1/image_raw" />
	    <param name="gt_type" value="trans" />
	    <param name="cvt2VGA" value="false" />
    </node>

</launch>
