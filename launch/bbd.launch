<launch>

	<rosparam command="load" file="$(env HOME)/workspace/data/bags/stereo/bbd/cam0.yaml" ns="/cam0"/>
	<rosparam command="load" file="$(env HOME)/workspace/data/bags/stereo/bbd/cam1.yaml" ns="/cam1"/>

	 <node name="manager_node" pkg="direct_stereo" type="manager_node" output="screen" >
	    <param name="cam0_topic" value="/cam_fl/image_raw" />
	    <param name="cam1_topic" value="/cam_fr/image_raw" />
	    <param name="cvt2VGA" value="false" />
    </node>

</launch>
