<launch>

  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find dsvo)/test/gazebo/asphalt.world"/>
  </include>

  <param name="robot_description" command="$(find xacro)/xacro $(find dsvo)/test/gazebo/stereo_camera.urdf.xacro" />

  <node name="spawn_model" pkg="gazebo_ros" type="spawn_model" output="screen" args="-urdf -param robot_description -z 1.5 -P 1.57 -model stereo_camera" />

</launch>
